   "source": [
    "#get a list of all the \"white\" and \"black\" names\n",
    "w = data[data.race=='w']\n",
    "b = data[data.race=='b']\n",
    "\n",
    "#determine sample size by length of each white and black name list\n",
    "n_w = len(w)\n",
    "n_b = len(b)\n",
    "\n",
    "#calcluate proportion of white and black sounding names tha received calls\n",
    "prop_w = np.sum(w.call) / len(w)\n",
    "prop_b = np.sum(b.call) / len(b)\n",
    "\n",
    "#Calculate the differnce between the two\n",
    "prop_diff = prop_w - prop_b\n",
    "phat = (np.sum(w.call) + np.sum(b.call)) / (len(w) + len(b))\n",
    "\n",
    "#Calculate the Z score and P-value\n",
    "z = prop_diff / np.sqrt(phat * (1 - phat) * ((1 / n_w) + (1 / n_b)))\n",
    "pval = stats.norm.cdf(-z) * 2\n",
    "print(\"Z score: {}\".format(z))\n",
    "print(\"P-value: {}\".format(pval))\n",
    "print('Percent of white names receiving callbacks: ',prop_w*100,'%')\n",
    "print('Percent of black names receiving callbacks: ',prop_b*100,'%')"
   ]
